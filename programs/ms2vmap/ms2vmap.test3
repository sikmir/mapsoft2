#!/bin/bash -fu

# General interface and option tests
# See also getopt module tests

P=${1:-./ms2vmap}

. ../test_lib.sh

assert "$P tmp import_mp" "Error: import_mp: file name expected" 1

rm -f tmp.db tmp.dbg

assert "$P tmp import_vmap1 test_data/n37-016.vmap" "" 0
assert "$P tmp export_vmap1 tmp.vmap" "" 0
assert_diff test_data/n37-016a.vmap tmp.vmap

